#include<stdio.h>
 main()
 {
 int arr1= 5, arr2 = 5, res, i, j;
    int a[5] = { 1, 2, 3, 4, 5 };
    int b[5] = { 6, 7, 8, 9, 10 };
    res= arr1+ arr2;
    int c[res];
    for (i = 0; i < arr1; i++) {
        c[i] = a[i];
    }
    for (i = 0, j = arr1;
         j < res && i < arr2; i++, j++) {
        c[j] = b[i];
    }
    // Array Elements After Merging
    for (i = 0; i < res; i++) {
        printf("%d ", c[i]);
    }
}
